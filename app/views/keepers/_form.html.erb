<%= form_for @keeper do |f| %>
    <div class="field">
        <%= f.label :name %>:
        <%= f.text_field :name %>
    </div><br>

    <div class="field">
        <%= f.hidden_field :zoo_id, value:"#{current_user.zoo_id}" %>
    </div><br>

    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>